	"slider-layout#team": {
		"children": [
			"flex-layout.row#group1",
			"flex-layout.row#group2",
			"flex-layout.row#group3"
		],
		"props": {
			"itemsPerPage": {
				"desktop": 1,
				"tablet": 1,
				"phone": 1
			},
			"infinite": true,
			"showPaginationDots": "never",
			"autoplay": {
				"timeout": 3000,
				"stopOnHover": true
			},
			"arrowSize": {
				"desktop": 25,
				"tablet": 20,
				"phone": 10
			},
			"blockClass": "sliderTeam"
		}
