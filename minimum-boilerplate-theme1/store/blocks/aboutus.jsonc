	//------------------------ABOUT US----------------------------------
	"stack-layout#aboutus": {
		"children": ["rich-text#aboutus"],

		"props": {
			"blockClass": "stackLayout"
		}
	},
	"rich-text#aboutus": {
		"props": {
			"textAlignment": "LEFT",
			"textPosition": "LEFT",
			"text": "# SOBRE NÓS  <span class='vtex-rich-text-0-x-heading--blink'></span>",
			"blockClass": "textAboutus"
		}
	},

	"flex-layout.row#title": {
		"children": ["rich-text#title"]
	},
	"rich-text#title": {
		"props": {
			"text": " ## Projetando o <span class='vtex-rich-text-0-x-heading--future'> futuro do comércio digital </span>",
			"textAlignment": "CENTER",
			"textPosition": "CENTER",
			"blockClass": "futureTitle"
		}
	},

	"flex-layout.row#future": {
		"children": ["flex-layout.col#textaboutus", "flex-layout.col#imageaboutus"],
		"props": {
			"blockClass": "futureRow"
		}
	},
	"flex-layout.col#textaboutus": {
		"children": ["rich-text#textaboutus"],
		"props": {
			"blockClass": "futureCol"
		}
	},
	"flex-layout.col#imageaboutus": {
		"children": ["image#textaboutus"],
		"props": {
			"blockClass": "image_future"
		}
	},

	"rich-text#textaboutus": {
		"props": {
			"textAlignment": "LEFT",
			"textPosition": "LEFT",
			"text": "Somos uma empresa de arquitetura e engenharia de software com 11 anos de história solucionando problemas de negócios com tecnologia.  \n \n  A **ACCT** passou por um rápido crescimento nos últimos 5 anos e hoje tem mais de 200 funcionários, em 3 países diferentes. \n \n Nós também temos o orgulho de dizer que a **ACCT** esteve envolvida em algumas das maiores implementações e projetos de e-commerce do mundo. \n \n Clientes globais como Unilever, Ab-Inbev, Carrefour e Whirpool nos escolheram para desenvolver seus produtos digitais porque confiam em nossa capacidade de fornecer soluções com a mais alta qualidade de design, engenharia e desenvolvimento",
			"blockClass": "description"
		}
	},
	"image#textaboutus": {
		"props": {
			"src": "assets/img-aboutus.jpg"
		}
	},

	//-----------------WHY ACCT?-------------------------------------
