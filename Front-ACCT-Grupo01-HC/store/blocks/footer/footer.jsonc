{
  "footer": {
    "blocks": ["footer-layout.desktop", "footer-layout.mobile"]
  },
  "footer-layout.desktop": {
    "children": [
      "flex-layout.row#footer-1-desktop"
     // "flex-layout.row#footer-2-desktop",
     // "flex-layout.row#footer-3-desktop"
    ]
  },
  "flex-layout.row#footer-1-desktop": {
    "children": [
      "flex-layout.col#footer-1-desktop",
      "flex-layout.col#footer-2-desktop",
      "flex-layout.col#footer-3-desktop",
      "flex-layout.col#footer-4-desktop",
      "flex-layout.col#footer-5-desktop"
      
     
    ],
    "props": {
      
    }
  },
  "flex-layout.col#footer-1-desktop": {
    "children": [
      
      "image#logofooter"
      //"social-networks"
    ]
  },
  "image#logofooter": {
    "props": {
      "src": "https://acct.global/wp-content/uploads/2021/07/selo-gptw-2021.svg",
      "maxHeight": 180,
      "alt": "Certificado"
      
    }
  },
  "flex-layout.col#footer-2-desktop": {
    "children": [
      "flex-layout.row#footer1",
      "flex-layout.row#footer2",
      "flex-layout.row#footer3"
      
     
    ]
  },
  "flex-layout.row#footer1": {
    "children": [
      "image#logo2footer"
    ]
  },
  "flex-layout.row#footer2": {
    "children": [
      "social-networks"
    ]
  },
  "image#logo2footer": {
    "props": {
      "src": "https://acct.global/wp-content/uploads/2021/06/logo-acct-black.svg",
      "maxHeight": 40,
      "alt": "Certificado"
      
    }
  },
  
  "social-networks": {
    "props": {
      "socialNetworks": [
        {
          "name": "Facebook",
          "url": "https://www.facebook.com/vtexonline/"
        },
        {
          "name": "Instagram",
          "url": "https://www.instagram.com/vtextruecloud/"
        },
        {
          "name": "Twitter",
          "url": "https://twitter.com/vtexonline"
        },
        {
          "name": "Youtube",
          "url": "https://www.youtube.com/user/VTEXTV"
        }
      ]
    }
  },
  "flex-layout.row#footer3": {
    "children": [
      "link#email"
    ]
  },
  
  "link#email": {
      "props": {
        "href": "/login?returnUrl={queryString.returnUrl}",
        "label": "contato@acct.global"
      }
  },
  "flex-layout.col#footer-3-desktop": {
    "children": [
      "flex-layout.row#1footer",
      "flex-layout.row#2footer"
    ],
    "props": {
      "blockClass": "credits"
    }
  },
  "flex-layout.row#1footer" : {
    "children": [
      "rich-text#footer"
    ]
  },
  "flex-layout.row#2footer" : {
    "children": [
      "rich-text#2footer"
    ]
  },
  "rich-text#footer": {
    "props": {
      "text": "All stock and product photos are from photos.icons8.com",
      "blockClass": "footer"
    }
  },
  "rich-text#2footer": {
    "props": {
      "text": "All stock and product photos are from photos.icons8.com",
      "blockClass": "footer"
    }
  },
  "flex-layout.col#footer-4-desktop": {
    "children": ["rich-text#footer2"],
    "props": {
      "blockClass": "credits"
    }
  },
  "rich-text#footer2": {
    "props": {
      "text": "Carrer Proven√ßa, 339 08037 - Barcelona",
      "blockClass": "footer"
    }
  },
  "flex-layout.col#footer-5-desktop": {
    "children": ["rich-text#footer3"],
    "props": {
      "blockClass": "credits"
    }
  },
  "rich-text#footer3": {
    "props": {
      "text": "12 East 49th Street NY 10017 - New York",
      "blockClass": "footer"
    }
  }

  
